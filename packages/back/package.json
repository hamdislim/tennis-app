{
  "name": "back",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "npx babel src -d build",
    "start": "nodemon src/index.ts",
    "test": "jest",
    "prettier": "prettier src --check",
    "prettier:fix": "prettier src --write",
    "lint": "eslint --ext .ts --ext .tsx src --color",
    "lint:changed": "eslint --ext .ts --ext .tsx src",
    "lint:fix": "eslint --fix --ext .ts --ext .tsx src",
    "precommit": "lint-staged && yarn test"
  },
  "dependencies": {
    "body-parser": "^1.20.0",
    "express": "^4.17.3"
  },
  "lint-staged": {
    "**/*.ts": [
      "yarn lint",
      "git add"
    ]
  },

  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.9",
    "@babel/node": "^7.16.8",
    "@babel/preset-env": "^7.16.11",
    "eslint": "^8.14.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "jest": "^28.0.0",
    "lint-staged": "^12.4.0",
    "nodemon": "^2.0.15",
    "prettier": "^2.6.2",
    "supertest": "^6.2.2",
    "ts-node": "^10.7.0"
  }
}
